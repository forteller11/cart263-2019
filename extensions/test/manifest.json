{
  "name": "Chrome Extension Example",
  "version": "1.0",
  "description": "test to build an extension",
  "manifest_version": 2,

  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },

  "browser_action": {
          "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png",
            "128": "images/icon128.png"
          },
          "default_title": "The End of History",
          "default_popup": "popup.html"
}
}
